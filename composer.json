{
    "name": "bunkermaster/multiproc",
    "description": "Simple multi processing with native PHP",
    "type": "library",
    "require-dev": {
        "symfony/var-dumper": "^3.3",
        "phpunit/phpunit": "^6.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "bunkermaster",
            "email": "bunkermaster@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Bunkermaster\\Multiproc\\Config\\": "src/Config",
            "Bunkermaster\\Multiproc\\Exception\\": "src/Exception",
            "Bunkermaster\\Multiproc\\Master\\": "src/Master",
            "Bunkermaster\\Multiproc\\Helper\\": "src/Helper",
